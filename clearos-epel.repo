[clearos-epel]
name=EPEL $releasever - $basearch
baseurl=http://download1.clearsdn.com/epel/$releasever/$basearch
    http://download2.clearsdn.com/epel/$releasever/$basearch
    http://download3.clearsdn.com/epel/$releasever/$basearch
    http://download4.clearsdn.com/epel/$releasever/$basearch
failovermethod=priority
enabled=0
gpgcheck=1
exclude=amavisd-* clamav-* clamd-* libc-client phpMyAdmin pptpd-* tnef-* uw-imap-* roundcubemail-*
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-$releasever
